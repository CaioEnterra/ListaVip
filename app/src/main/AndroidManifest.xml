<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ascom_unitins.listavip">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/porra"
        android:label="@string/app_name"
        android:roundIcon="@drawable/porra"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".View.TelaPrincipal"
            android:screenOrientation="portrait"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".View.TelaSobre"

             />
        <activity
            android:name=".View.TelaCadastroEvento"
            android:screenOrientation="portrait"
             />
        <activity
            android:name=".View.TelaCadastroPessoas"
            android:screenOrientation="portrait"
             />
        <activity
            android:name=".View.TelaListaEvento"
            />
        <activity android:name=".View.TelaListaPessoas"></activity>
    </application>

</manifest>